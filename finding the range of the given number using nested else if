#include<stdio.h>
int main()
{
  int a;
  printf("enter a positive number");
  scanf("%d",&a);
  if(a<=10)
  {
      printf("the given number is in the range of 1 to 10");
  }
  else if(a>10&&a<=20)
  {
      printf("the given number is in the range of 11 to 20 ");
      
  }
  else if(a>20&&a<=30)
  {
      printf("the given number is in the range of 21 to 30");
  }
  else if(a>30&&a<=40)
  {
      printf("the given number is in the range of 31 to 40");
  }
  else
  printf("the given is greater than 40");
}
