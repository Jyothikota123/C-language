#include<stdio.h>
#include<string.h>
void main()
{
    char name[10][10];
    int n;
    printf("enter number of names:");
    scanf("%d",&n);
    for( int i=0;i<n;i++)
    {
        printf("enter the names:");
        scanf("%s",name[i]);
    }
    printf("the names are as follows...\n");
    for(int i=0;i<n;i++)
    {
        printf("\nnames%d. %s",i+1,name[i]);
    }
}
