#include<stdio.h>
int main()
{
 char ch;
 printf("enter a character");
 scanf("%c",&ch);
 switch(ch)
 {
     case 'A':
     case 'E':
     case 'I':
     case 'O':
     case 'U':
     case 'a':
     case 'e':
     case 'i':
     case 'o':
     case 'u':
     printf("\n the given character is vowel");
     break;
     default:printf("the given character is consonant");
 }
     
 }
