#include <stdio.h>
#include<string.h>
int main() 
{
 char str1[53],str2[35];
 printf("enter a password");
 fgets(str1,sizeof(stdin),stdin);
 printf("re enter password");
 fgets(str2,sizeof(stdin),stdin);

if(strcmp(str1,str2)==0)
{printf("password is matched");
}
else{
    printf("not matched");
}
}
