#include<stdio.h>
int main()
{
 int x[100],i,n,small=x[0];
 printf("enter the size of an array:");
 scanf("%d",&n);
 for(i=0;i<n;i++)
 {
  printf("enter an element:");
  scanf("%d",&x[i]);
 }
 small=x[0];
 for(i=0;i<n;i++)
 {
  if(small>x[i])
  {
 small=x[i];
 }
 }
 printf("\n smallest element is %d",small);
}
